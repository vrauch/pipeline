<% if @note.errors.empty? %>
  if ($("#startup_notes").find(".empty_block").length > 0) {
    $("#startup_notes").html("<%= j(render @note.assignee.present? ? 'index_tasks' : 'index_notes') %>");
  }
  $("<%= j render 'note', note: @note %>").prependTo("#accordion");
  $.controlMethods.hidePanel("add_note");
  $("#notification").html("<%= j(render('layouts/flash_message')) %>")
  $('.top_notification').addClass('sm_size').showFlashMessage()
<% else %>
  $("#note_form").html("<%= j render 'new' %>");
  $.myMethods.calendar_init();
<% end %>
$.controlMethods.removePlaceholder();