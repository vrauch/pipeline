<div class="head_content has_btn hidden-xs">
  <h1 class="h1_title">Tasks</h1>
  <div class="subtitle">Assign a task to an Evol8tion team member for this startup.</div>
  <%= link_to '+ Add Task', new_note_path(startup_id: @startup.id, type: 'task'), remote: true, class:'btn btn_add' %>
</div>
<%= link_to '+ Add Task', new_note_path(startup_id: @startup.id, type: 'task'),
            class:'btn btn_open_panel btn_add btn_mobile visible-xs',
            data: { panel: 'add_note' }, remote: true %>
<div class="accordion_holder">
  <div class="panel-group startup_notes_table" id="accordion" role="tablist" aria-multiselectable="true">
    <%= render partial: 'notes/note', collection: @tasks %>
  </div>
</div>