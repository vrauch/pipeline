<div id="list_startups">
  <% if @startups.empty? %>
    <%= render @list.favorite ? 'empty_favorites' : 'empty_lists' %>
  <% else %>
    <%= render '/startups/filter_startups' %>
    <div class="list_holder">
      <h2 class="mobile_title visible-xs"><%= @list.name %></h2>
      <div class="boxer hovered styled_list">
        <div class="box_row clearfix box_head hidden-xs">
          <%= render 'startups/header' %>
        </div>
        <div id="list_startups_table">
          <%= render partial: '/startups/startup', collection: @startups %>
        </div>
        <div id='list-startups-paginator'>
          <%= paginate @startups, remote: true %>
        </div>
      </div>
    </div>
  <% end %>
</div>
