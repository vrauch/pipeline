<% if @list.startups.size > 0 %>
  $("#notification").html("<%= j(render('layouts/flash_message')) %>");
  $('.top_notification').showFlashMessage();
  el = $("#list_startups_table");
  el.html("<%= j render partial: '/startups/startup', collection: @list.startups %>");
  $.controlMethods.removePlaceholder();
<% else %>
  $("#list_startups").html("<%= j(render(@list.favorite ? 'empty_favorites' : 'empty_lists')) %>");
<% end %>
