<% if @invite.startup? %>
  container = $(".invite_startup.clicked");
  if (container.find(".tip_holder").length == 0) {
    container.addClass('visible').append("<%= j render 'edit' %>");
  }
<% else %>
  $("#invite-<%= @invite.id %>").find(".resend_invite_wrap").html("<%= j render 'edit' %>").fadeIn(400);
<% end %>

