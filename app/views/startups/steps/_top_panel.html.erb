<ul class="step_name_list step_name_list_padd md_size clearfix">
  <li class="pull-left name_main_step"><%= @startup.new_record? ? 'ADD' : 'EDIT' %> STARTUP</li>
  <% if @startup.prefilled_brief? || @startup.new_record? %>
    <% Startup::STEPS.each do |step| %>
      <li class="<%= @startup.current_step == step ? 'active' : '' %>"><%= step.upcase %></li>
    <% end %>
  <% end %>
</ul>